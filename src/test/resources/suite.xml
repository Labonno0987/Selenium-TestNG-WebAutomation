<?xml version="1.0" encoding="UTF-8" ?>
<suite name="FinancialWebTestSuite">

    <test name="User Get Congratulations Email after Registration">
        <classes>
            <class name="testRunner.RegistrationTestRunner"></class>
        </classes>
    </test>

    <test name="Attampt to Reset Password">
        <classes>
           <class name="testRunner.ResetPasswordTestRunner"></class>
        </classes>
    </test>

    <test name="User Login with New Password">
        <classes>
            <class name="testRunner.UserLoginNewPasswordTestRunner"></class>
        </classes>
    </test>

    <test name="Add 2 items:Item-1 with all fields,Item-2 with only mandatory fields">
        <classes>
            <class name="testRunner.AddItemTestRunner"></class>
        </classes>
    </test>

    <test name="Update user Gmail with a new Gmail">
        <classes>
            <class name="testRunner.UpdateUserGmailTestRunner"></class>
        </classes>
    </test>


    <test name="Login with Updated Gmail is Successful and Login with Old Gmail is Failed">
        <classes>
            <class name="testRunner.UserLoginNewGmailTestRunner"></class>
        </classes>
    </test>


    <test name="Admin Login From Terminal and Searching the Updated User Gmail from Admin Dashboard">
        <classes>
            <class name="testRunner.AdminLoginAndSearchEmailTestRunner"></class>
        </classes>
    </test>

    <test name="Register Users getting Data from CSV file">
        <classes>
            <class name="testRunner.CSVuserRegTestRunner"></class>
        </classes>
    </test>

    <test name="Admin Get all the User from User Table">
        <classes>
            <class name="testRunner.GetAllUserInfoTestRunner"></class>
        </classes>
    </test>


</suite>